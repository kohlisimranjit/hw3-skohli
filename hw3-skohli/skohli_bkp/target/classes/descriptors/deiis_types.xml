<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>deiis_types</name>
  <description/>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>edu.cmu.deiis.types.Annotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>casProcessorId</name>
          <description>The unique identifier of the CAS processor that added this annotation to the CAS</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>confidence</name>
          <description>The confidence level assigned to the Annotation by the CAS processor that created it.</description>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Token</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.NGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>elements</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.types.Annotation</elementType>
        </featureDescription>
        <featureDescription>
          <name>elementType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Answer</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>isCorrect</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.Question</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.types.AnswerScore</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answer</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.types.Answer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.subTypes.AnnotatedAnswer</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.AnswerScore</supertypeName>
    <features>
        <featureDescription>
          <name>text</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answerId</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>sentiment</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.subTypes.AnnotatedQuestion</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Question</supertypeName>
      <features>
        <featureDescription>
          <name>text</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>sentiment</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.subTypes.AnnotatedToken</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>tokenText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      <featureDescription>
          <name>pos</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.subTypes.TokenizedSentence</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>annotatedTokens</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.subTypes.AnnotatedToken</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.subTypes.AnnotatedNGram</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.NGram</supertypeName>
      <features>
        <featureDescription>
          <name>nGramToken</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.subTypes.Document</name>
      <description/>
      <supertypeName>edu.cmu.deiis.types.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>question</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.subTypes.AnnotatedQuestion</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>answers</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.subTypes.AnnotatedAnswer</elementType>
          <multipleReferencesAllowed>false</multipleReferencesAllowed>
        </featureDescription>
      <featureDescription>
          <name>threshold</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>edu.cmu.deiis.subTypes.TokenizedDocument</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>tokenizedQuestion</name>
          <description/>
          <rangeTypeName>edu.cmu.deiis.subTypes.TokenizedSentence</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>tokenizedAnswers</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>edu.cmu.deiis.subTypes.TokenizedSentence</elementType>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>edu.cmu.deiis.subTypes.NGramMatrix</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>matrix</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>uima.cas.FSArray</elementType>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
